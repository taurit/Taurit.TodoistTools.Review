{
    "compilerOptions": {
        "noImplicitAny": true,
        "noEmitOnError": true,
        "removeComments": true,
        "sourceMap": true,
        "target": "ES2022",
        "outFile": "bundle.js",
        "strictBindCallApply": true,
        "strictPropertyInitialization": false,
        "strictFunctionTypes": true,
        "strictNullChecks": true
    },
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
